    "MyFastAPIFunctionLambdaVersioniEuboA5fL1wnBI5oTd5dQM5atoiRVXThA88D20OU": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "MyFastAPIFunctionLambdaFunction"
        },
        "CodeSha256": "3a81b0500e8f5beb55427fddf3160892e56f37c23281b9be0be0955aeb477c47"
      }
    },
    "MyFastAPIFunctionLambdaFunctionUrl": {
      "Type": "AWS::Lambda::Url",
