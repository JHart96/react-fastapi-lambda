    "MyFastAPIFunctionLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "MyFastAPIFunctionLambdaVersioniEuboA5fL1wnBI5oTd5dQM5atoiRVXThA88D20OU"
      },
      "Export": {
